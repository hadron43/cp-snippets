	"Binary Exponentiation": {
		"prefix": "_binpow",
		"body": [
			"ll binpow(ll a, ll b) {",
				"\ta %= modulo;",
				"\tll res = 1;",
				"\twhile (b > 0) {",
					"\t\tif (b & 1)",
						"\t\t\tres = res * a % modulo;",
					"\t\ta = a * a % modulo;",
					"\t\tb >>= 1;",
				"\t}",
				"\treturn res;",
			"}",
			""
		],
		"description": "Binary Exponentiation With Modulo"
	},

